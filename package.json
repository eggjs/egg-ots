{
  "name": "egg-ots",
  "version": "1.0.0",
  "description": "aliyun ots plugin for egg",
  "eggPlugin": {
    "name": "ots"
  },
  "keywords": [
    "egg",
    "plugin"
  ],
  "dependencies": {
    "ots2": "^1.2.2"
  },
  "devDependencies": {
    "autod": "2",
    "egg": "*",
    "egg-bin": "1",
    "egg-ci": "1",
    "egg-mock": "*",
    "eslint": "3",
    "eslint-config-egg": "3",
    "intelli-espower-loader": "1",
    "power-assert": "1",
    "supertest": "2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test -r intelli-espower-loader",
    "cov": "egg-bin cov -r intelli-espower-loader",
    "lint": "eslint --ext js . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "4, 6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-ots.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-ots#readme",
  "author": "tianyi.jiangty",
  "license": "MIT"
}
